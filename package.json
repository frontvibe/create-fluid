{
	"name": "create-fluid",
	"version": "0.0.0",
	"type": "module",
	"author": "frontvibe",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/frontvibe/create-fluid.git"
	},
	"exports": {
		".": "./create-fluid.mjs"
	},
	"main": "./create-fluid.mjs",
	"bin": {
		"create-fluid": "./create-fluid.mjs"
	},
	"scripts": {
		"build": "tsup src/index.ts --format cjs,esm --dts",
