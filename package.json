		"@changesets/cli": "^2.28.1",
		"@types/node": "^22.13.10",
		"arg": "^5.0.2",
		"prettier": "^3.5.3",
		"strip-ansi": "^7.1.0",
		"tsup": "^8.4.0",
		"typescript": "^5.8.2"
	},
	"engines": {
		"node": "^18.17.1 || ^20.3.0 || >=22.0.0"
	},
	"publishConfig": {
